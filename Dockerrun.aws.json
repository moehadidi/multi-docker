{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
   {
     "name": "client",
     "image": "moehadidi/mutli-client",
     "hostname": "client",
     "essential": false,
     "memory": 128
   },
   {
     "name": "server",
     "image": "moehadidi/mutli-server",
     "hostname": "api",
     "essential": false,
     "memory": 128
   },
   {
     "name": "worker",
     "image": "moehadidi/mutli-worker",
     "hostname": "worker",
     "essential": false,
     "memory": 128
   },
   {
     "name": "nginx",
     "image": "moehadidi/mutli-nginx",
     "hostname": "nginx",
     "essential": true,
     "portMappings": [
       {
         "hostPort": 80,
         "containerPort": 80
       }
     ],
     "links": ["client", "server"],
     "memory": 128
   }
 ]
}
